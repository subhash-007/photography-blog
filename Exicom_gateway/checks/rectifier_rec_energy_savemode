#!/usr/bin/python
import binascii
"""
vlan Poller script.
This is part of device application. 
Poller script determines the vlan. 
poller script takes the snmp value of OID .1.3.6.1.4.1.161.19.3.2.1.55.0 from snmp agent of device at specific interval.
all ports status are sent to device application 
"""
# ######################################################################
# Function : check_exicom_model_no_invent
#
# Parameters: info (SNMP Output) _no_params(No Parameters)
#
# Output: service state and plugin output
# #####################################################################
def check_rectifier_rec_energy_savemode(item, _no_params, info):
    """
                check_exicom_model_no_invent function calculates vlan

                Args:
                        item (str) Specific item on SNMP output on which we want to filter results
                
                Kwargs:
                        params (tuple) Check parameters for critical and warning state of service

                Returns:
                        state (int) :
                                        0 : OK
                                        1 : Warning
                                        2: Critical
                    3: unknown
                        infotext(string):
                                        plugin output
                                        Example : OK - vlan=1;;;;

                        Raises:
                               Exception
    """
    state = 3
    infotext = "unknown_value"
    rec_energy_savemode = None
    try:
        if info:
            if info[0][0]:
                rec_energy_savemode = info[0][0]
		try:
		    rec_energy_savemode = float(rec_energy_savemode)
		except Exception,e:
		    rec_energy_savemode = info[0][0].replace(' ','@')
                state = 0
                infotext = "energy_savemode=%s" % rec_energy_savemode
    except Exception,e:
        infotext = "unknown_value"
    return (state,infotext,[("energy_savemode",rec_energy_savemode)])



check_info["rectifier_rec_energy_savemode"] = {
    'check_function':          check_rectifier_rec_energy_savemode,
    'service_description':     'rectifier_rec_energy_savemode',
    'has_perfdata':         True,
    'snmp_info':               ('.1.3.6.1.4.1.38016.14.2.8', ['5.0']),
    'snmp_scan_function':      lambda oid: "m1000" in oid(".1.3.6.1.4.1.38016.14.1.1.0").lower(),
}


